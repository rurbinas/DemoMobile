{
  "name": "react-onsenui",
  "version": "1.4.0",
  "description": "Onsen UI - React Components for Hybrid Cordova/PhoneGap Apps with Material Design and iOS UI components",
  "scripts": {
    "build": "npm run lint && webpack --config webpack.prod.config.js",
    "prepublish": "npm run build",
    "postpublish": "npm run gen-docs",
    "lint": "eslint --ext .js,.jsx src",
    "lint-fix": "eslint --ext .js,.jsx src --fix",
    "dev": "webpack-dev-server --progress --host 0.0.0.0 --config webpack.dev.config.js",
    "test": "karma start",
    "gen-docs": "node ./docgen.js"
  },
  "main": "dist/react-onsenui.js",
  "files": [
    "dist/react-onsenui.*",
    "src"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/OnsenUI/OnsenUI.git"
  },
  "keywords": [
    "react",
    "react-component",
    "phonegap",
    "cordova",
    "hybrid",
    "app",
    "mobile",
    "ui",
    "onsen",
    "onsenui"
  ],
  "author": "Onsen UI Team <team@monaca.io>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/OnsenUI/OnsenUI/issues"
  },
  "homepage": "https://onsen.io/react/",
  "devDependencies": {
    "babel-core": "^6.17.0",
    "babel-loader": "^7",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-3": "^6.22.0",
    "chai": "^3.5.0",
    "colors": "^1.1.2",
    "css-loader": "0.26.0",
    "eslint": "^3.7.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.0.1",
    "file-loader": "^0.9.0",
    "filter-files": "^0.4.0",
    "karma": "^1.3.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-mocha": "^1.2.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.1.2",
    "prompt": "^1.0.0",
    "react": "^15.5.3",
    "react-docgen": "^2.13.0",
    "react-dom": "^15.5.3",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^1.16.2"
  },
  "peerDependencies": {
    "onsenui": "~2.4.0",
    "react": "^15.5.3",
    "react-dom": "^15.5.3"
  },
  "dependencies": {
    "prop-types": "^15.5.10"
  }
}
